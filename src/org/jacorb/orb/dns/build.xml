<?xml version="1.0"?>

<!DOCTYPE project [ <!ENTITY common SYSTEM "file:../../../../../etc/common.xml"> ]>

<project name="dns" default="all" basedir="../../../../..">

  <!-- ==================================================== -->
  <!--             dns lookup build file                    -->
  <!-- $Id$ -->
  <!-- ==================================================== -->

   &common;

   <target name="all" depends="init,dns"/>

   <target name="dns">
      <antcall target="jacorb-javac">
         <param name="javac-includes" 
                value="org/jacorb/orb/dns/DNSLookup.java,org/jacorb/orb/dns/DNSLookupDelegate.java,org/jacorb/orb/dns/JdkDelegateImpl.java"/>
      </antcall>
   </target>


   <target name="clean" depends="init">
        <delete dir="${classdir}/org/jacorb/orb/dns" />
   </target>
</project>
